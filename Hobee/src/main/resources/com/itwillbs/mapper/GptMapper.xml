<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itwillbs.mapper.GptMapper">
	
	<!-- MyBatis XML -->
	<insert id="insetInterest">
	    INSERT INTO user_interest (user_id, interest_id)
	    SELECT #{user_id}, interest_id 
	    FROM interest 
	    WHERE interest_name IN
	    <foreach collection="tags" item="tag" open="(" separator="," close=")">
	        #{tag}
	    </foreach>
	</insert>
	
			


</mapper>